    "query": {
        "from": {
            "table": "Magazines\\Magazines\\Users\\Code\\Tables\\Users",
            "alias": "mmu",
            "name": "#__magazines_magazines_users"
        },
        "select": [
            "mmu.id",
            "mmu.user_id",
            "uu.name AS user_id_name",
            "mmu.magazine_id",
            "mm.title AS magazine_id_title",
            "mmu.created_by",
            "mmu.date_created",
            "mmu.modified_by",
            "mmu.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "mmu.user_id = uu.id"
            },
            {
                "table": "Magazines\\Magazines\\Code\\Tables\\Magazines",
                "alias": "mm",
                "name": "#__magazines_magazines",
                "on": "mmu.magazine_id = mm.id"
            }
        ]
    }
